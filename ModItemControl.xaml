<UserControl x:Class="MgsVModsMananer.ModItemControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MgsVModsMananer"
             mc:Ignorable="d" 
             Height="30">
    <Grid HorizontalAlignment="Left">
        <CheckBox x:Name="checkBox" Content="CheckBox" Margin="0" VerticalAlignment="Center" Checked="checkBox_Checked" Unchecked="checkBox_Unchecked"/>
    </Grid>
</UserControl>
